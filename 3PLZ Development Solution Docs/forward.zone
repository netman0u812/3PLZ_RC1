
$TTL 86400
@   IN  SOA ns1.example.com. admin.example.com. (
        2025090501 ; Serial
        3600       ; Refresh
        1800       ; Retry
        604800     ; Expire
        86400 )    ; Minimum TTL

    IN  NS  ns1.example.com.
ns1 IN  A   192.168.0.1
webserver1 IN A 192.168.0.10
oldserver IN A 192.168.0.20
alias1 IN CNAME webserver1.example.com.
alias2 IN CNAME oldserver.example.com.
info1 IN TXT "Site hosted in Canada"
info2 IN TXT "Deprecated entry"
